{
  "name": "uyeh-tech-server",
  "version": "6.0.0",
  "description": "Complete UYEH TECH Backend Server v6.0 - Admin Dashboard System with User Management, Orders, Products, Blog, Coupons, and Analytics",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "express",
    "mongodb",
    "admin-dashboard",
    "ecommerce",
    "blog",
    "authentication",
    "api",
    "nodejs",
    "backend",
    "rest-api",
    "jwt",
    "payment-gateway",
    "flutterwave",
    "email-verification",
    "otp",
    "coupon-system",
    "user-management",
    "analytics"
  ],
  "author": "UYEH TECH <uyehtech@gmail.com> (https://uyehtech.com)",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uyehtech/backend-server.git"
  },
  "bugs": {
    "url": "https://github.com/uyehtech/backend-server/issues",
    "email": "support@uyehtech.com"
  },
  "homepage": "https://uyehtech.com",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^2.0.2",
    "socket.io": "^4.8.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules"
    ],
    "verbose": true,
    "execMap": {
      "js": "node"
    },
    "watch": [
      "server.js",
      ".env"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json"
  },
  "config": {
    "port": 3000,
    "mongodbMemoryServer": {
      "version": "latest"
    }
  },
  "type": "commonjs"
}
